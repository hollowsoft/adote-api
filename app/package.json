{
  "name": "oreo",
  "version": "1.0.0",
  "description": "",
  "author": "hollow",
  "private": true,
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"spec/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"spec/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,spec}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,spec}/**/*.ts\"",
    "start": "bun ./src/main.ts --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.663.0",
    "@aws-sdk/client-ses": "^3.662.0",
    "@fastify/compress": "^6.5.0",
    "@fastify/helmet": "^11.1.1",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.7",
    "@nestjs/throttler": "^5.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.2.2",
    "nodemailer": "^6.9.15",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.7",
    "@types/bun": "^1.1.14",
    "@types/jest": "29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/passport-jwt": "^3.0.12",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
